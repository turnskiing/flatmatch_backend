{
  "name": "flatmatch-backend",
  "version": "1.0.0",
  "description": "A web server providing backend functionality to the flatmatch frontend",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "clean-node": "rimraf node_modules/*",
    "swagger-autogen": "node swagger.ts",
    "copy-assets": "ts-node tools/copyAssets",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "format": "tsfmt -r",
    "tsc": "tsc",
    "build": "npm-run-all clean swagger-autogen lint format tsc copy-assets",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts --exec npm run dev:start",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "flatmatch",
    "mern",
    "js",
    "javascrips",
    "express"
  ],
  "author": "Manuel Klettner",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "http": "*",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.9",
    "mongoose-unique-validator": "^2.0.3",
    "ncp": "^2.0.0",
    "request": "^2.81.0",
    "swagger-ui-express": "^4.1.6",
    "typescript-formatter": "^7.2.2",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^15.0.3",
    "@types/shelljs": "^0.8.8",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^13.1.4",
    "fs-extra": "^10.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "swagger-autogen": "^2.9.1",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  }
}
